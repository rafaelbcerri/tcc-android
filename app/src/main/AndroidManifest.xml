<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rafaelbcerri.psychologist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity android:name=".SplashScreen"
            android:theme="@style/SplashTheme"
            android:noHistory="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Login"  android:theme="@style/Login">
        </activity>

        <activity android:name=".SignUpPatient"  android:theme="@style/Login">
        </activity>

        <activity android:name=".SignUpPsychologist"  android:theme="@style/Login">
        </activity>

        <activity android:name=".HomePsychologist"  android:theme="@style/AppTheme">
        </activity>

        <activity android:name=".HomePatient"  android:theme="@style/AppTheme">
        </activity>

        <activity android:name=".Search"  android:theme="@style/AppTheme">
        </activity>


        <activity android:name=".EditPsychologist"  android:theme="@style/AppTheme">
        </activity>


    </application>

</manifest>